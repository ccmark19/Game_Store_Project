@model IEnumerable<gaimz.Models.user_event>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eventID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eventID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.user_event_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.user_event_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.user_event_ID })
        </td>
    </tr>
}

</table>
